extends ../templates/contents.pug

block headVars
    - var title = "iGEM Memes"
    - var tagline = "A sample page for the theme."
    - var requireMathJax = true


block main
    - var fs = require('fs')
    - var memenames = fs.readdirSync('src/assets/img/memes')

    each name in memenames.reverse()
        +meme(name, "")

