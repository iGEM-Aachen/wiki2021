{
    "name": "",
    "version": "1.0.0",
    "description": "Wiki for iGEM 2021 Aachen",
    "main": "index.js",
    "scripts": {
        "preprocess": "python utils/preprocess.py",
        "nav": "python utils/nav.py",
        "citations": "python utils/citations.py",
        "puglint": "glob-exec './src/**/*.pug' -- \"pug-lint {{files.join(' ')}}\"",
        "custom_tests": "python utils/test.py",
        "test": "npm-run-all puglint custom_tests",
        "prestart": "npm-run-all preprocess nav citations custom_tests",
        "start": "webpack-dev-server --config webpack.development.js --open --host 0.0.0.0",
        "build": "webpack --config webpack.production.js",
        "build:all": "npm-run-all prestart build",
        "postbuild": "python utils/postbuild.py",
        "wikisync": "python wikisync.py",
        "publish": "npm-run-all build:all wikisync",
        "preprocess:win": "python utils/preprocess.py",
        "nav:win": "python utils/nav.py",
        "citations:win": "python utils/citations.py",
        "custom_tests:win": "python utils/test.py",
        "server:win": "webpack-dev-server --config webpack.development.js --open --host 0.0.0.0",
        "start:win": "npm-run-all preprocess:win nav:win citations:win custom_tests:win server:win",
        "build:win": "webpack --config webpack.production.js",
        "build:all:win": "npm-run-all prestart:win build:win",
        "postbuild:win": "python utils/postbuild.py",
        "wikisync:win": "python wikisync.py",
        "publish:win": "npm-run-all build:all:win wikisync:win"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com//.git"
    },
    "author": "Malte Schwerin",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com///issues"
    },
    "homepage": "https://github.com//#readme",
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^5.14.0",
        "bootstrap": "^4.5.2",
        "bootstrap-reboot": "^4.4.1",
        "clean-webpack-plugin": "^3.0.0",
        "crossref": "^1.2.1",
        "css-loader": "^3.6.0",
        "extract-css-chunks-webpack-plugin": "^4.7.5",
        "file-loader": "^6.0.0",
        "glob-exec": "^0.1.1",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "image-webpack-loader": "^6.0.0",
        "jquery": "^3.5.1",
        "jstransformer-markdown-it": "^2.1.0",
        "jstransformer-mathjax": "0.0.3",
        "jstransformer-verbatim": "^1.1.1",
        "markdown-it": "^11.0.0",
        "markdown-it-bracketed-spans": "^1.0.1",
        "markdown-it-multimd-table": "^4.0.3",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "npm-run-all": "^4.1.5",
        "popper.js": "^1.16.1",
        "pug": "^2.0.4",
        "pug-html-loader": "^1.1.5",
        "pug-lint": "^2.6.0",
        "pug-loader": "^2.4.0",
        "sass": "^1.26.10",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2"
